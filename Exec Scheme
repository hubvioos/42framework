		|
		|> CORE -> __construct()
		|				|
		|				|> REQUEST -> __construct()
		|								|
		|								|> ROUTER -> routeToApp()
		|												|> defines MODULE and ACTION
		|
		|> CORE	-> execute()
		|				|
		|				|> MODULE -> execute(ACTION)
		|				|> defines VIEW
		|				
		|> MODULE -> display()
						|
						|> RESPONSE -> send()
						|> render()
								|
								|> includes Layout
								|> includes View